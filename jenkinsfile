pipeline {
  agent any 
  stages {
      stage('Test') {
      when {
                expression {
                    env.BRANCH_NAME.contains("test")
            }
            }
      steps { 
            echo "${BUILD_NUMBER}"
            echo "${env.BRANCH_NAME}"
            echo "branch_name="+ env.BRANCH_NAME
            echo "GIT BRANCH="+ env.GIT_BRANCH
           }
       }
       }
       }
